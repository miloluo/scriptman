#### Command Definition ####
#MACHINFO=/usr/contrib/bin/wrongcmd
MACHINFO=/usr/contrib/bin/machinfo
UNAME=/usr/bin/uname
UPTIME=/usr/bin/uptime
TOP=/usr/bin/top
ULIMIT=/usr/bin/ulimit
VMSTAT=/usr/bin/vmstat
IOSTAT=/usr/bin/iostat

LANSCAN=/usr/sbin/lanscan
NETSTAT=/usr/bin/netstat
BDF=/usr/bin/bdf
SWAPINFO=/usr/sbin/swapinfo
VGDISPLAY=/usr/sbin/vgdisplay
IOSCAN=/usr/sbin/ioscan
SHOW_PATCHES=/usr/contrib/bin/show_patches
TEST=adflajflaj
############################

CMDLIST='$MACHINFO $UNAME $UPTIME $TOP $ULIMIT $VMSTAT $IOSTAT $LANSCAN $BDF $SWAPINFO $VGDISPLAY $IOSCAN $SHOW_PATCHES'
CMDLIST1='$MACHINFO $TEST'


##### Validate commands ####
echo "Validating the OS command..."

for c in $CMDLIST1
$c > /dev/null 2>&1
if [ `echo $?` -gt 0 ]
then
   echo "$c is not working, please check!" | tee cmd_err.log
   sleep 3
fi

sleep 10;


##### Execute those commands ####

ksh -c "$MACHINFO">>/tmp/aticheck/11.txt
ksh -c "$UNAME -a">>/tmp/aticheck/11.txt
ksh -c "$UPTIEM">>/tmp/aticheck/11.txt
ksh -c "$TOP -s 1 -d 1 -n 40">>/tmp/aticheck/11.txt
ksh -c "$ULIMIT -a">>/tmp/aticheck/11.txt
ksh -c "$VMSTAT 2 5">>/tmp/aticheck/11.txt
ksh -c "$IOSTAT 2 5">>/tmp/aticheck/11.txt
ksh -c "$LANSCAN -i | cut -d\" \" -f1 | xargs -n1 /usr/sbin/ifconfig">>/tmp/aticheck/11.txt
ksh -c "$NETSTAT -in">>/tmp/aticheck/11.txt
ksh -c "$NETSTAT -rn">>/tmp/aticheck/11.txt
ksh -c "$BDF">>/tmp/aticheck/11.txt
ksh -c "$SWAPINFO -tm">>/tmp/aticheck/11.txt
ksh -c "$VGDISPLAY -v">>/tmp/aticheck/11.txt
ksh -c "$IOSCAN -fnkC processor">>/tmp/aticheck/11.txt
ksh -c "$SHOW_PATCHES">>/tmp/aticheck/11.txt
ksh -c "cat /etc/hosts">>/tmp/aticheck/11.txt
ksh -c "tail -1000 /var/adm/syslog/syslog.log">>/tmp/aticheck/11.txt